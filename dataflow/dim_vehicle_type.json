{
	"name": "dim_vehicle_type",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Snowflake_ny_ausin",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTable_dim_vehicle_type",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select2"
				},
				{
					"name": "union1"
				},
				{
					"name": "select3"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CRASH_ID as string,",
				"          UNITS_INVOLVED as string,",
				"          DI_CREATEDDATE as string,",
				"          DI_WORKFLOWFILENAME as string,",
				"          DI_SOURCENAME as string,",
				"          PID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     format: 'table') ~> source1",
				"source(output(",
				"          VEHICLE_SK as decimal(38,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     query: 'select NVL(MAX(VEHICLE_SK),0) as VEHICLE_SK  from DIM_vehicle_type',",
				"     format: 'query') ~> source2",
				"derivedColumn1 select(mapColumn(",
				"          VEHICLE_SK,",
				"          vehicle_type = UNITS_INVOLVED,",
				"          DI_created_dt,",
				"          PID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select3, source2 union(byName: true)~> union1",
				"source1 select(mapColumn(",
				"          CRASH_ID,",
				"          UNITS_INVOLVED,",
				"          PID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"union1 derive(DI_created_dt = currentTimestamp()) ~> derivedColumn1",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}