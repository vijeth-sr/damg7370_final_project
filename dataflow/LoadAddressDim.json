{
	"name": "LoadAddressDim",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeStagiing",
						"type": "DatasetReference"
					},
					"name": "AustinSTG"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeStagiing",
						"type": "DatasetReference"
					},
					"name": "ChicagoSTG"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeStagiing",
						"type": "DatasetReference"
					},
					"name": "NycSTG"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeStagiing",
						"type": "DatasetReference"
					},
					"name": "MontgomerySTG"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeStagiing",
						"type": "DatasetReference"
					},
					"name": "dimAddress"
				}
			],
			"transformations": [
				{
					"name": "selectTX"
				},
				{
					"name": "selectIL"
				},
				{
					"name": "selectNYC"
				},
				{
					"name": "selectMO"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "derivedColumn4"
				},
				{
					"name": "union1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn5"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ADDRESS_SK as string,",
				"          STREET_NAME as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          DI_WORKFLOW_NAME as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> AustinSTG",
				"source(output(",
				"          ADDRESS_SK as string,",
				"          STREET_NAME as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          DI_WORKFLOW_NAME as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> ChicagoSTG",
				"source(output(",
				"          ADDRESS_SK as string,",
				"          STREET_NAME as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          DI_WORKFLOW_NAME as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> NycSTG",
				"source(output(",
				"          ADDRESS_SK as string,",
				"          STREET_NAME as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          DI_WORKFLOW_NAME as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> MontgomerySTG",
				"AustinSTG select(mapColumn(",
				"          STREET_ADDRESS,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          CITY,",
				"          COUNTRY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectTX",
				"ChicagoSTG select(mapColumn(",
				"          STREET_NAME,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          CITY,",
				"          COUNTRY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectIL",
				"NycSTG select(mapColumn(",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          STREET_NAME,",
				"          ZIP_CODE,",
				"          CITY,",
				"          COUNTRY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectNYC",
				"MontgomerySTG select(mapColumn(",
				"          STREET_NAME,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          LOCATION,",
				"          CITY,",
				"          COUNTRY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectMO",
				"selectTX derive(STREET_ADDRESS = trim(STREET_ADDRESS),",
				"          LATITUDE = toFloat(LATITUDE),",
				"          LONGITUDE = toFloat(LONGITUDE),",
				"          CITY = trim(CITY),",
				"          COUNTRY = trim(COUNTRY)) ~> derivedColumn1",
				"selectIL derive(STREET_ADDRESS = trim(STREET_NAME),",
				"          CITY = trim(CITY),",
				"          COUNTRY = trim(COUNTRY),",
				"          LATITUDE = toFloat(LATITUDE),",
				"          LONGITUDE = toFloat(LONGITUDE)) ~> derivedColumn2",
				"selectNYC derive(STREET_ADDRESS = trim(STREET_NAME),",
				"          LATITUDE = toFloat(LATITUDE),",
				"          LONGITUDE = toFloat(LONGITUDE),",
				"          CITY = trim(CITY),",
				"          COUNTRY = trim(COUNTRY)) ~> derivedColumn3",
				"selectMO derive(COUNTRY = trim(upper(COUNTRY)),",
				"          CITY = trim(upper(CITY)),",
				"          LATITUDE = toFloat(LATITUDE),",
				"          STREET_ADDRESS = upper(trim(STREET_NAME)),",
				"          LONGITUDE = toFloat(LONGITUDE)) ~> derivedColumn4",
				"derivedColumn1, derivedColumn2, derivedColumn3, derivedColumn4 union(byName: true)~> union1",
				"union1 keyGenerate(output(ADDRESS_SK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(di_create_date = currentDate(),",
				"          di_workflow = \"Address_Integration_Workflow\") ~> derivedColumn5",
				"derivedColumn5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ADDRESS_SK as string,",
				"          STREET_NAME as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          DI_WORKFLOW_NAME as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          ADDRESS_SK,",
				"          STREET_NAME,",
				"          CITY,",
				"          COUNTRY,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          DI_WORKFLOW_NAME = di_workflow,",
				"          DI_CREATED_DT = di_create_date",
				"     )) ~> dimAddress"
			]
		}
	}
}