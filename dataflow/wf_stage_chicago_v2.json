{
	"name": "wf_stage_chicago_v2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "stage_chicago_v1",
						"type": "DatasetReference"
					},
					"name": "sourcesnowflakechicagostage"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "stage_chicago_v2",
						"type": "DatasetReference"
					},
					"name": "sinkStageChicago"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "union1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_WORKFLOW_NAME as string (\"STG_CHICAGO_V2\")",
				"}",
				"source(output(",
				"          CRASH_RECORD_ID as string,",
				"          CRASH_DATE_EST_I as string,",
				"          CRASH_DATE as timestamp,",
				"          POSTED_SPEED_LIMIT as integer,",
				"          TRAFFIC_CONTROL_DEVICE as string,",
				"          DEVICE_CONDITION as string,",
				"          WEATHER_CONDITION as string,",
				"          LIGHTING_CONDITION as string,",
				"          FIRST_CRASH_TYPE as string,",
				"          TRAFFICWAY_TYPE as string,",
				"          LANE_CNT as integer,",
				"          ALIGNMENT as string,",
				"          ROADWAY_SURFACE_COND as string,",
				"          ROAD_DEFECT as string,",
				"          REPORT_TYPE as string,",
				"          CRASH_TYPE as string,",
				"          INTERSECTION_RELATED_I as string,",
				"          NOT_RIGHT_OF_WAY_I as string,",
				"          HIT_AND_RUN_I as string,",
				"          DAMAGE as string,",
				"          DATE_POLICE_NOTIFIED as timestamp,",
				"          PRIM_CONTRIBUTORY_CAUSE as string,",
				"          SEC_CONTRIBUTORY_CAUSE as string,",
				"          STREET_NO as integer,",
				"          STREET_DIRECTION as string,",
				"          STREET_NAME as string,",
				"          BEAT_OF_OCCURRENCE as integer,",
				"          PHOTOS_TAKEN_I as string,",
				"          STATEMENTS_TAKEN_I as string,",
				"          DOORING_I as string,",
				"          WORK_ZONE_I as string,",
				"          WORK_ZONE_TYPE as string,",
				"          WORKERS_PRESENT_I as string,",
				"          NUM_UNITS as decimal(38,0),",
				"          MOST_SEVERE_INJURY as string,",
				"          INJURIES_TOTAL as integer,",
				"          INJURIES_FATAL as integer,",
				"          INJURIES_INCAPACITATING as integer,",
				"          INJURIES_NON_INCAPACITATING as integer,",
				"          INJURIES_REPORTED_NOT_EVIDENT as integer,",
				"          INJURIES_NO_INDICATION as integer,",
				"          INJURIES_UNKNOWN as integer,",
				"          CRASH_HOUR as integer,",
				"          CRASH_DAY_OF_WEEK as integer,",
				"          CRASH_MONTH as integer,",
				"          LATITUDE as double,",
				"          LONGITUDE as double,",
				"          LOCATION as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as date,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourcesnowflakechicagostage",
				"sourcesnowflakechicagostage derive(CRASH_DATE = toDate(CRASH_DATE),",
				"          CRASH_TIME = substring(toString(CRASH_DATE), 11, 9),",
				"          STREET_NAME = iif(",
				"    STREET_NO == 0 && !isNull(STREET_NAME) && STREET_NAME != '%UNKNOWN%', upper(trim(STREET_NAME)),",
				"    iif(",
				"        STREET_NO == 0, 'UNKNOWN',",
				"        iif(",
				"            isNull((STREET_NAME)), 'UNKNOWN',",
				"            iif(",
				"                isNull(STREET_NO) && !isNull(STREET_NAME), upper(trim(STREET_NAME)),",
				"                toString(STREET_NO) + ' ' + upper(trim(STREET_NAME))",
				"            )",
				"        )",
				"    )",
				"),",
				"          INJURIES_TOTAL = coalesce(INJURIES_TOTAL, 0),",
				"          INJURIES_FATAL = coalesce(INJURIES_FATAL, 0),",
				"          LATITUDE = iif(isNull(LATITUDE), toDouble(0), LATITUDE),",
				"          LONGITUDE = iif(isNull(LONGITUDE), toDouble(0), LONGITUDE)) ~> derivedColumn1",
				"derivedColumn1 derive(CONTRIBUTORY_FACTOR = upper(trim(PRIM_CONTRIBUTORY_CAUSE))) ~> derivedColumn2",
				"derivedColumn2 derive(CONTRIBUTORY_FACTOR = upper(trim(SEC_CONTRIBUTORY_CAUSE))) ~> derivedColumn3",
				"derivedColumn3, derivedColumn2 union(byName: true)~> union1",
				"union1 filter(!isNull(CONTRIBUTORY_FACTOR)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CRASH_RECORD_ID as string,",
				"          CRASH_DATE as string,",
				"          CRASH_TIME as string,",
				"          PRIM_CONTRIBUTORY_CAUSE as string,",
				"          SEC_CONTRIBUTORY_CAUSE as string,",
				"          STREET_NAME as string,",
				"          INJURIES_TOTAL as string,",
				"          INJURIES_FATAL as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          CONTRIBUTING_FACTOR_MERGED as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          CRASH_RECORD_ID,",
				"          CRASH_DATE,",
				"          CRASH_TIME,",
				"          PRIM_CONTRIBUTORY_CAUSE,",
				"          SEC_CONTRIBUTORY_CAUSE,",
				"          STREET_NAME,",
				"          INJURIES_TOTAL,",
				"          INJURIES_FATAL,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          CONTRIBUTING_FACTOR_MERGED,",
				"          CITY,",
				"          COUNTRY,",
				"          DI_SOURCE_NAME,",
				"          DI_CREATED_DATE,",
				"          DI_WORKFLOW_NAME",
				"     )) ~> sinkStageChicago"
			]
		}
	}
}