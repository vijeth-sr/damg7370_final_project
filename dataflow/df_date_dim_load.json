{
	"name": "df_date_dim_load",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText3",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "snowflakeDimDate",
						"type": "DatasetReference"
					},
					"name": "sinkDimDate"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn3"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_WORKFLOW_NAME as string (\"DATE_DIM\")",
				"}",
				"source(output(",
				"          GeneratedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(date = toDate(GeneratedDate),",
				"          utc_date = toTimestamp(GeneratedDate)) ~> derivedColumn1",
				"derivedColumn1 derive(Full_Date_In_Words = concat(toString(dayOfMonth(utc_date))) + ' ' + concat(iif(month(utc_date) == 1, 'January',",
				"iif(month(utc_date) == 2, 'February',",
				"iif(month(utc_date) == 3, 'March',",
				"iif(month(utc_date) == 4, 'April',",
				"iif(month(utc_date) == 5, 'May',",
				"iif(month(utc_date) == 6, 'June',",
				"iif(month(utc_date) == 7, 'July',",
				"iif(month(utc_date) == 8, 'August',",
				"iif(month(utc_date) == 9, 'September',",
				"iif(month(utc_date) == 10, 'October',",
				"iif(month(utc_date) == 11, 'November',",
				"iif(month(utc_date) == 12, 'December',",
				"'Unknown'))))))))))))) + ' ' + concat(toString(year(utc_date))),",
				"          Day_Of_Month = dayOfMonth(utc_date),",
				"          Month = month(utc_date),",
				"          Month_Of_Year = iif(month(utc_date) == 1, 'January',",
				"iif(month(utc_date) == 2, 'February',",
				"iif(month(utc_date) == 3, 'March',",
				"iif(month(utc_date) == 4, 'April',",
				"iif(month(utc_date) == 5, 'May',",
				"iif(month(utc_date) == 6, 'June',",
				"iif(month(utc_date) == 7, 'July',",
				"iif(month(utc_date) == 8, 'August',",
				"iif(month(utc_date) == 9, 'September',",
				"iif(month(utc_date) == 10, 'October',",
				"iif(month(utc_date) == 11, 'November',",
				"iif(month(utc_date) == 12, 'December',",
				"'Unknown')))))))))))),",
				"          Year = year(utc_date),",
				"          Day_Of_Week = dayOfWeek(utc_date),",
				"          DI_CREATED_DATE = toString(currentDate(), 'yyyy-MM-dd'),",
				"          DI_WORKFLOW_NAME = $DI_WORKFLOW_NAME) ~> derivedColumn2",
				"derivedColumn3 keyGenerate(output(date_sk as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"derivedColumn2 derive(Season = iif(Month == 12, 'Winter',",
				"iif(Month == 1, 'Winter',",
				"iif(Month == 2, 'Winter',",
				"iif(Month == 3, 'Spring',",
				"iif(Month == 4, 'Spring',",
				"iif(Month == 5, 'Spring',",
				"iif(Month == 6, 'Summer',",
				"iif(Month == 7, 'Summer',",
				"iif(Month == 8, 'Summer',",
				"iif(Month == 9, 'Fall',",
				"iif(Month == 10, 'Fall',",
				"iif(Month == 11, 'Fall',",
				"'Unknown')))))))))))),",
				"          Weekend_Indicator = iif(Day_Of_Week == 1, 'Yes',",
				"iif(Day_Of_Week == 7, 'Yes',",
				"'No'))) ~> derivedColumn3",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DATE_SK as string,",
				"          DATE as string,",
				"          DATE_IN_WORDS as string,",
				"          DAY_OF_MONTH as string,",
				"          MONTH as string,",
				"          MONTH_OF_YEAR as string,",
				"          YEAR as string,",
				"          DAY_OF_WEEK as string,",
				"          SEASON as string,",
				"          WEEKEND_INDICATOR as string,",
				"          DI_CREATED_DT as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          DATE_SK = date_sk,",
				"          DATE = date,",
				"          DATE_IN_WORDS = Full_Date_In_Words,",
				"          DAY_OF_MONTH = Day_Of_Month,",
				"          MONTH = Month,",
				"          MONTH_OF_YEAR = Year,",
				"          YEAR = Month_Of_Year,",
				"          DAY_OF_WEEK = Day_Of_Week,",
				"          SEASON = Season,",
				"          WEEKEND_INDICATOR = Weekend_Indicator,",
				"          DI_CREATED_DT = DI_CREATED_DATE,",
				"          DI_WORKFLOW_NAME",
				"     )) ~> sinkDimDate"
			]
		}
	}
}