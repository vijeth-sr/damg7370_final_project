{
	"name": "wf_stage_montgomery_v2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "stg_montgomery_v1",
						"type": "DatasetReference"
					},
					"name": "stageMontgomeryRaw"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "stg_montgomery_v2",
						"type": "DatasetReference"
					},
					"name": "sinkMontgomeryV2"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_WORKFLOW_NAME as string (\"mittul-adf\")",
				"}",
				"source(output(",
				"          REPORT_NUMBER as string,",
				"          LOCAL_CASE_NUMBER as string,",
				"          AGENCY_NAME as string,",
				"          ACRS_REPORT_TYPE as string,",
				"          CRASH_DATE_TIME as string,",
				"          HIT_RUN as string,",
				"          ROUTE_TYPE as string,",
				"          LANE_DIRECTION as string,",
				"          LANE_TYPE as string,",
				"          NUMBER_OF_LANES as string,",
				"          DIRECTION as string,",
				"          DISTANCE as string,",
				"          DISTANCE_UNIT as string,",
				"          ROAD_GRADE as string,",
				"          ROAD_NAME as string,",
				"          CROSS_STREET_NAME as string,",
				"          OFF_ROAD_DESCRIPTION as string,",
				"          MUNICIPALITY as string,",
				"          RELATED_NON_MOTORIST as string,",
				"          AT_FAULT as string,",
				"          COLLISION_TYPE as string,",
				"          WEATHER as string,",
				"          SURFACE_CONDITION as string,",
				"          LIGHT as string,",
				"          TRAFFIC_CONTROL as string,",
				"          DRIVER_SUBSTANCE_ABUSE as string,",
				"          NON_MOTORIST_SUBSTANCE_ABUSE as string,",
				"          FIRST_HARMFUL_EVENT as string,",
				"          SECOND_HARMFUL_EVENT as string,",
				"          JUNCTION as string,",
				"          INTERSECTION_TYPE as string,",
				"          ROAD_ALIGNMENT as string,",
				"          ROAD_CONDITION as string,",
				"          ROAD_DIVISION as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          LOCATION as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> stageMontgomeryRaw",
				"stageMontgomeryRaw filter(!isNull(LOCATION)) ~> filter1",
				"filter1 derive(REPORT_NUMBER = upper(trim(REPORT_NUMBER)),",
				"          LOCAL_CASE_NUMBER = upper(trim(LOCAL_CASE_NUMBER)),",
				"          CRASH_DATE = toDate(trim(CRASH_DATE_TIME)),",
				"          CRASH_TIME = trim(substring(CRASH_DATE_TIME, 11, 9)),",
				"          STREET_NAME = iif(isNull(ROAD_NAME), 'N/A', upper(trim(ROAD_NAME))),",
				"          CONTRIBUTORY_FACTOR = iif(isNull(COLLISION_TYPE), 'N/A', upper(trim(COLLISION_TYPE)))) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          REPORT_NUMBER as string,",
				"          LOCAL_CASE_NUMBER as string,",
				"          CRASH_DATE as string,",
				"          CRASH_TIME as string,",
				"          STREET_NAME as string,",
				"          CONTRIBUTORY_FACTOR as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          LOCATION as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          REPORT_NUMBER,",
				"          LOCAL_CASE_NUMBER,",
				"          CRASH_DATE,",
				"          CRASH_TIME,",
				"          STREET_NAME,",
				"          CONTRIBUTORY_FACTOR,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          LOCATION,",
				"          CITY,",
				"          COUNTRY,",
				"          DI_SOURCE_NAME,",
				"          DI_CREATED_DATE,",
				"          DI_WORKFLOW_NAME",
				"     )) ~> sinkMontgomeryV2"
			]
		}
	}
}