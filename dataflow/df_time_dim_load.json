{
	"name": "df_time_dim_load",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "delimitedtimeDim",
						"type": "DatasetReference"
					},
					"name": "sourceTimeDim"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "sinkTimeDim"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "surrogateKey1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_WORKFLOW_NAME as string (\"mittul-adf\")",
				"}",
				"source(output(",
				"          time_sk as string,",
				"          time as string,",
				"          hour as integer,",
				"          minute as integer,",
				"          am_or_pm as string,",
				"          time_category as string,",
				"          DI_CreateDate as string,",
				"          DI_Workflow_FileName as string,",
				"          pid as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceTimeDim",
				"sourceTimeDim derive(DI_CREATED_DATE = toString(currentDate(), 'yyyy-MM-dd'),",
				"          DI_WORKFLOW_NAME = $DI_WORKFLOW_NAME) ~> derivedColumn1",
				"derivedColumn1 keyGenerate(output(Time_sk as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TIME_SK as string,",
				"          TIME as string,",
				"          HOUR as string,",
				"          MINUTE as string,",
				"          AM_OR_PM as string,",
				"          TIME_CATEGORY as string,",
				"          DI_CREATED_DT as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TIME_SK = surrogateKey1@Time_sk,",
				"          TIME = time,",
				"          HOUR = hour,",
				"          MINUTE = minute,",
				"          AM_OR_PM = am_or_pm,",
				"          TIME_CATEGORY = time_category,",
				"          DI_CREATED_DT = DI_CREATED_DATE,",
				"          DI_WORKFLOW_NAME",
				"     )) ~> sinkTimeDim"
			]
		}
	}
}